
<h1> Home </h1>
  <% if current_user %>
  <%= "Hello "+current_user.email %> <br>
    <%= link_to 'Logout', '/logout', :data => { :confirm => 'Are you sure?' }%>
  <p>
    <%= button_to 'Users', user_index_path, method: :get %>
  </p>
  <p>
    <%= button_to 'Artist',  artists_path, method: :get %>
  </p>
  <% else %>
    <%= link_to 'Signup', new_user_path%> <br>
    <%= link_to 'Login', '/login'%>
<% end %>